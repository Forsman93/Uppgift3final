
@{
    ViewBag.Title = "Log In";
}

<div class="login-box">

    <header class='login-box-header'>
        <h1>Frilansare? Log in!</h1>        
    </header>

    <div class="container">

        <form action="" name="login-frilanser">

            <label for="email">Epostadress</label>
            <input type="text" name="epostadress" id="emailaddress" placeholder="email" />

            <label for="password">Lösenord</label>
            <input type="password" name="password" id="password" />

            <p><a class="btn btn-default" @Html.ActionLink("Login", "Index", "Profile")</a></p>
            

        </form>


    </div>
</div>